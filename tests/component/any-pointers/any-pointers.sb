#include "tests-common.sb"

fun funReturningAnyPointer() -> any *
{
    return 0xabcdefffabcdefff as any *;
}

fun printAnyPointer(any *a)
{
    u64 asNumber = a as u64;
    printHex(asNumber, 1);
}

fun main()
{
    any *hereBeDragons = funReturningAnyPointer();
    u64 dragons = hereBeDragons as u64;
    printHex(dragons, 1);
    printAnyPointer(hereBeDragons);
    exit();
}
