#include "tests-common.sb"


fun main()
{
    printStr("This string has a newline\n" as u8 *);
    printStr("This string should have an 'A' after the colon:!\bA\n" as u8 *);
    printStr("\tThis string should be indented by one tab\n" as u8 *);
